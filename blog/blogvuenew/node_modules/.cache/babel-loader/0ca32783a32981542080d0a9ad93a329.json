/assets/css/base.scss\";\r\n@import \"../assets/css/mixin.scss\";\r\n@import \"../assets/css/variable.scss\";\r\n.articles{\r\n  width: 80%;\r\n  margin: 0 auto;\r\n  padding-top: 20px;\r\n}\r\n.active{\r\n  @include font_active_color();\r\n}\r\nh1{\r\n  margin: 20px auto;\r\n  font-size: 20px;\r\n  padding: 20px;\r\n  border-radius: 10px;\r\n  @include font_color();\r\n  @include bg_color();\r\n}\r\n.top{\r\n  width: 100%;\r\n  height: 180px;\r\n  padding: 10px;\r\n  display: flex;\r\n  // justify-content: space-around;\r\n  align-items: center;\r\n  margin-bottom: 20px;\r\n  border: 1px solid #fff;\r\n  border-radius: 5px;\r\n  @include font_color();\r\n  @include bg_color();\r\n  li{\r\n    cursor: pointer;\r\n    padding: 5px 8px;\r\n    border-radius: 5px;\r\n    border: 1px solid #000;\r\n    @include border_color();\r\n    margin-left: 20px;\r\n  }\r\n  li:hover{\r\n    @include font_active_color();\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}