base.scss\";\r\n@import \"../../assets/css/mixin.scss\";\r\n.life{\r\n  width: 80%;\r\n  margin: 0 auto;\r\n  margin-top: 20px;\r\n  padding: 0 10px;\r\n  box-sizing: border-box;\r\n  @include font_color();\r\n  @include bg_color();\r\n}\r\n.top{\r\n  margin-bottom: 20px;\r\n}\r\n.bottom{\r\n  width: 100%;\r\n  height: 600px;\r\n  margin-top: 20px;\r\n}\r\n.timeline{\r\n  width: 100%;\r\n  padding-bottom: 10px;\r\n  padding-left: 30px;\r\n  position: relative;\r\n  p{\r\n    span{\r\n      font-weight: bold;\r\n      margin-right: 5px;\r\n    }\r\n  }\r\n  .line{\r\n    width: 1px;\r\n    height: 100%;\r\n    background-color: #ccc;\r\n    position: absolute;\r\n    top: 0;\r\n    left: 8px;\r\n  }\r\n  .icon{\r\n    position: absolute;\r\n    top: 0;\r\n    left: -6px;\r\n    z-index: 99;\r\n    font-size: 30px;\r\n    color: #000;\r\n  }\r\n  .reply{\r\n    cursor: pointer;\r\n    font-size: 12px;\r\n    margin: 5px 0;\r\n  }\r\n  .rep{\r\n      padding-left: 40px;\r\n      position: relative;\r\n      .inicon{\r\n        position: absolute;\r\n        top: 0;\r\n        left: 0px;\r\n        z-index: 99;\r\n        font-size: 30px;\r\n        color: #000;\r\n      }\r\n    }\r\n}\r\n</style>"],"sourceRoot":"src/components/Manage"}]}