AA;AACA,KAzGA;AA0GA,IAAA,SA1GA,uBA0GA;AACA,UAAA,QAAA,GAAA,SAAA,QAAA,GAAA;AACA,aAAA,CAAA,GAAA,CAAA;AACA,OAFA;;AAGA,WAAA,KAAA,GAAA,IAAA,QAAA,EAAA;AACA,UAAA,GAAA,GAAA,IAAA,GAAA,CAAA,GAAA,EAAA;AACA,MAAA,GAAA,CAAA,GAAA,CAAA,KAAA,KAAA,EAAA,GAAA,EAAA,CAAA,EAAA,IAAA,EAAA,IAAA,CAAA,GAAA;AACA,KAjHA;AAkHA,IAAA,OAlHA,qBAkHA;AACA,MAAA,qBAAA,CAAA,KAAA,OAAA,CAAA,CADA,CAEA;;AACA,WAAA,IAAA,CAAA,QAAA,CAAA,CAAA,GAAA,KAAA,KAAA,CAAA,CAAA;AACA,WAAA,IAAA,CAAA,QAAA,CAAA,CAAA,IAAA,IAAA;AACA,WAAA,IAAA,CAAA,QAAA,CAAA,CAAA,IAAA,IAAA;AACA,WAAA,QAAA,CAAA,KAAA;AACA,WAAA,QAAA,CAAA,MAAA,CAAA,KAAA,KAAA,EAAA,KAAA,MAAA,EAPA,CAOA;AACA;AA1HA,GAfA;AA2IA,EAAA,OA3IA,qBA2IA;AACA,SAAA,IAAA;AACA,SAAA,OAAA;AACA;AA9IA,CAAA","sourcesContent":["<template>\r\n  <div id=\"page\">\r\n    <div class=\"enter\">\r\n      <button type=\"success\" @click=\"manage\">Login</button>\r\n      <button type=\"success\" @click=\"welcome\">Enter to Blog</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as THREE from \"three\";\r\nimport * as dat from \"dat.gui\";\r\nexport default {\r\n  name: \"Page\",\r\n  data() {\r\n    return {\r\n      camera: null,\r\n      scene: null,\r\n      renderer: null,\r\n      light: null,\r\n      cube: null,\r\n      mesh: null,\r\n      width: null,\r\n      height: null,\r\n      param: null,\r\n    };\r\n  },\r\n  methods: {\r\n    welcome() {\r\n      this.$router.push({ path: \"/blog\" });\r\n    },\r\n    manage() {\r\n      this.$router.push({ path: \"/edit\" });\r\n    },\r\n    init() {\r\n      this.initThree();\r\n      this.CreateGUI();\r\n      this.initCamera();\r\n      this.initScene();\r\n      this.initLight();\r\n      this.initObject();\r\n    },\r\n    initThree() {\r\n      this.width = document.documentElement.clientWidth;\r\n      this.height = document.documentElement.clientHeight;\r\n      this.renderer = new THREE.WebGLRenderer({ antialias: true });\r\n      this.renderer.setClearColor(0x333333, 1); // 背景（颜色，透明度）一定要是16位的颜色\r\n      this.renderer.setSize(this.width, this.height);\r\n      page.appendChild(this.renderer.domElement);\r\n    },\r\n    initCamera() {\r\n      this.camera = new THREE.PerspectiveCamera(\r\n        45,\r\n        this.width / this.height,\r\n        1,\r\n        1000\r\n      );\r\n      this.camera.position.set(0, 0, 800);\r\n      this.camera.lookAt(0, 0, 0);\r\n    },\r\n    initScene() {\r\n      this.scene = new THREE.Scene();\r\n    },\r\n    initLight() {\r\n      this.light = new THREE.DirectionalLight(0xffffff, 1.0, 0);\r\n      this.light.position.set(100, 100, 200);\r\n      this.scene.add(this.light);\r\n    },\r\n    initObject() {\r\n      let geometry = new THREE.BufferGeometry();\r\n\r\n      //类型数组创建顶点位置position数据\r\n      var vertices = new Float32Array([\r\n        0,\r\n        0,\r\n        0, //顶点1坐标\r\n        50,\r\n        0,\r\n        0, //顶点2坐标\r\n        0,\r\n        100,\r\n        0, //顶点3坐标\r\n        0,\r\n        0,\r\n        10, //顶点4坐标\r\n        0,\r\n        0,\r\n        100, //顶点5坐标\r\n        50,\r\n        0,\r\n        10, //顶点6坐标\r\n        0,\r\n        10,\r\n        10, //顶点1坐标\r\n        0,\r\n        10,\r\n        60, //顶点2坐标\r\n        0,\r\n        100,\r\n        10, //顶点3坐标\r\n      ]);\r\n      // 创建属性缓冲区对象\r\n      var attribue = new THREE.BufferAttribute(vertices, 3); //3个为一组，作为一个顶点的xyz坐标\r\n      // 设置几何体attributes属性的位置属性\r\n      geometry.attributes.position = attribue;\r\n      var colors = new Float32Array([\r\n        1, 0, 0, //顶点1颜色\r\n        0, 1, 0, //顶点2颜色\r\n        0, 0, 1, //顶点3颜色\r\n\r\n        1, 1, 0, //顶点4颜色\r\n        0, 1, 1, //顶点5颜色\r\n        1, 0, 1, //顶点6颜色\r\n        1, 0, 1, //顶点4颜色\r\n        0, 1, 1, //顶点5颜色\r\n        1, 0, 0, //顶点6颜色\r\n      ]);\r\n      // 设置几何体attributes属性的颜色color属性\r\n      geometry.attributes.color = new THREE.BufferAttribute(colors, 3); //3个为一组,表示一个顶点的颜色数据RGB\r\n      //材质对象\r\n      var material = new THREE.PointsMaterial({\r\n        // 使用顶点颜色数据渲染模型，不需要再定义color属性\r\n        // color: 0xff0000,\r\n        vertexColors: THREE.VertexColors, //以顶点颜色为准\r\n        side: THREE.DoubleSide, //两面可见\r\n      });\r\n\r\n      this.mesh = new THREE.Mesh(geometry, material);\r\n      this.scene.add(this.mesh);\r\n      // let axisHelper = new THREE.AxesHelper(40);\r\n      // this.scene.add(axisHelper);\r\n      this.mesh.position.set(0, 0, 0);\r\n    },\r\n    CreateGUI() {\r\n      var paramObj = function () {\r\n        this.x = 0;\r\n      };\r\n      this.param = new paramObj();\r\n      var gui = new dat.GUI();\r\n      gui.add(this.param, \"x\", 0, 1080).name(\"X\");\r\n    },\r\n    animate() {\r\n      requestAnimationFrame(this.animate);\r\n      // console.log(this.mesh.position.x);\r\n      this.mesh.position.x = this.param.x;\r\n      this.mesh.rotation.x += 0.01;\r\n      this.mesh.rotation.y += 0.02;\r\n      this.renderer.clear();\r\n      this.renderer.render(this.scene, this.camera); //再次渲染\r\n    },\r\n  },\r\n  mounted() {\r\n    this.init();\r\n    this.animate();\r\n  },\r\n};\r\n</script>\r\n<style scoped lang=\"scss\">\r\n#page {\r\n  width: 100%;\r\n  height: 100%;\r\n  position: relative;\r\n  overflow: hidden;\r\n  .enter {\r\n    width: 100%;\r\n    padding: 0 20px;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    position: absolute;\r\n    bottom: 50%;\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}